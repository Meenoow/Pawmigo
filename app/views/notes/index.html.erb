<!-- app/views/notes/index.html.erb -->
<h1>All Notes</h1>
<ul>
  <% @notes.each do |note| %>
    <li><%= note.content %></li>
  <% end %>
</ul>

<div class="card bg-dark text-white" style="width: 20rem;">
  <img src="https://lh3.googleusercontent.com/p/AF1QipNcvxXF0_ovUidm0nXfqWP83k2PNYUP7zgIViZU=s1360-w1360-h1020" class="card-img opacity-50"  alt="...">
  <div class="card-img-overlay">
    <h5 class="card-title">Pet Name</h5>
    <h6>Note Name</h6>
    <h6>Note Date</h6>
    <button type="button" class="btn btn-dark">View this Note</button>
  </div>
</div>
