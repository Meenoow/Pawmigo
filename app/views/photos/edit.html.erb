<!-- app/views/photos/edit.html.erb -->
<h1>Edit Photo</h1>
<%= form_for @photo, url: photo_path(@photo), method: :patch, html: { multipart: true } do |f| %>
  <!-- Photo fields and file input here -->
  <%= f.label :content %>
  <%= f.text_area :content %>

  <!-- File upload field -->
  <%= f.label :image %>
  <%= f.file_field :image %>

  <%= f.submit "Update Photo" %>
<% end %>

=begin code for linking this page to the PETS page
 <%= link_to "Edit Photo", edit_photo_path(@photo) %> 
=end
