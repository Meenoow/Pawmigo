<!-- app/views/schedules/index.html.erb -->
<h1>All Schedules</h1>
<ul>
  <% @schedules.each do |schedule| %>
    <li><%= schedule.content %></li>
  <% end %>
</ul>

<div class="card bg-dark text-white" style="width: 20rem;">
  <img src="https://lh3.googleusercontent.com/p/AF1QipNcvxXF0_ovUidm0nXfqWP83k2PNYUP7zgIViZU=s1360-w1360-h1020" class="card-img opacity-50"  alt="...">
  <div class="card-img-overlay">
    <h5 class="card-title">Pet Name</h5>
    <h6>Schedule Name</h6>
    <h6>Schedule Date</h6>
    <button type="button" class="btn btn-dark">View this Schedule</button>
  </div>
</div>
